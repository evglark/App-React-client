{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./src",
        "experimentalDecorators": true,
        "jsx": "react",
        "lib": [
            "es5",
            "es6",
            "es7",
            "dom"
        ],
        "module": "esNext",
        "moduleResolution": "node",
        "target": "esNext",
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "*/node_modules//*"
    ]
}
